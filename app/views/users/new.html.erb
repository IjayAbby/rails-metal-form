<%= form_for @user, url: {action: "create"}, html: {class: ""} do |f| %>
    UserName: <%= f.text_field :username %><br><br>
    Email: <%= f.text_field :email, placeholder: 'example@example.com' %><br><br>
    Password: <%= f.text_field :password, placeholder: '**********' %><br><br>
    <%= f.submit "Submit" %>
<% end %>



